
<h1>Listings</h1>


<div id='card'>
<%= link_to "New Listing", new_listing_path%>
</div>
<%= turbo_stream_from "listings", id: "listings" %>



  <% @listings.each do |listing| %>
  <div class="card" style="width:100%;">
  <ul class="list-group">
    <li class="list-group-item">
    <%= render listing %>


      <%= link_to "Show this listing", listing %>
      </li>
      </ul>
      </div>
  

  <% end %>
</ul>
