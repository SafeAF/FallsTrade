<div id="<%= dom_id service_provider %>">
  <p>
    <strong>Name:</strong>
    <%= service_provider.name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= service_provider.company %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= service_provider.bio %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= service_provider.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= service_provider.email %>
  </p>

  <p>
    <strong>User:</strong>
    <%= service_provider.user_id %>
  </p>

</div>
